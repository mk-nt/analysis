# Create a simple model (unclamped)

x ~ dnExponential(1)
a[1] ~ dnExponential(x)
a[1].clamp(1)
a[2] ~ dnInverse(dnExponential(x))
a[2].clamp(1)
mymodel = model(x)

# Create a move vector and a monitor vector
moves[1] = mvScale( x, lambda = 1.0, weight = 1.0 )
monitors[1] = mnScreen(printgen = 10, x)

# Create an mcmc object
mymcmcObject = mcmc( mymodel, monitors, moves )

# Run a short analysis
mymcmcObject.run( generations = 400 )
a[1].lnProbability()
a[2].lnProbability()
